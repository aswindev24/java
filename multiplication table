class Array
{
    public static void main(String args[])
    {
        int[] a;
        a=new int[11];
        int i,j;
        for(i=1;i<11;i++)
        {
           j=4*i;
           a[i]=j;
           
        }
        for(i=1;i<11;i++)
        {
        System.out.println("4"+"*"+i+"="+a[i]);
        }
    }
}
